{
  "name": "metadata-transforms",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/metadata-transforms/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/metadata-transforms",
        "main": "packages/metadata-transforms/src/index.ts",
        "tsConfig": "packages/metadata-transforms/tsconfig.lib.json",
        "assets": ["packages/metadata-transforms/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/metadata-transforms/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/metadata-transforms/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
