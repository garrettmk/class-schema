{
  "name": "metadata-rules",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/metadata-rules/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/metadata-rules",
        "main": "packages/metadata-rules/src/index.ts",
        "tsConfig": "packages/metadata-rules/tsconfig.lib.json",
        "assets": ["packages/metadata-rules/*.md"]
      }
    },
    "document": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx typedoc --options packages/metadata-rules/typedoc.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/metadata-rules/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/metadata-rules/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
